function create_trail(obj,dir,freq)
	local ofst = 0
	if(obj.flp) then
		ofst = 8
	end
	for i = 1,freq do
		local cx = obj.x
		local cy = obj.y
		if(dir == ⬅️) then
			if(obj.flp) then
				cx += 8
			end
			cy += 4 + rnd(4)
		elseif(dir == ⬆️) then
			cy += 8
			cx += 2 + rnd(2)
			if(not(obj.flp)) then
				cx -= 1
			else
				cx += 1
			end
		end
		add(trail.parts,{
			x = cx,
			y = cy,
			r = rnd(2),
			c = 6,
			speed =1+rnd(2),
			l = 10,
		})
	end
end

