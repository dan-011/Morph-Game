function gen_jump()
	local cg = coyote_ground(plr)
	local jump_again = cg and (time() - plr.jump_time < 0.3)
	if((btnp(⬆️) and cg) or jump_again) then
		plr.vy = get_jump_height(plr)
	elseif(btnp(⬆️)) then
	 plr.jump_time = time()
	end
end

function gen_vel()
		if(btn(➡️)) then
			plr.gvx = get_speed(plr)
			plr.flp = false
		elseif(btn(⬅️)) then
			plr.gvx = get_speed(plr) * -1
			plr.flp = true
		else
			plr.gvx = 0
		end
		gen_jump()
end

function rhino_vel()
		if(not(energy_bar.decr)) then
			gen_vel()
			return
		end
		if(plr.flp) then
			plr.vx = plr[plr.mode].charge_speed * -1
		else
			plr.vx = plr[plr.mode].charge_speed
		end
		plr.gvx = 0
		gen_jump()
end

function frog_vel()
		if(btn(➡️)) then
			plr.gvx = get_speed(plr)
			plr.flp = false
		elseif(btn(⬅️)) then
			plr.gvx = get_speed(plr) * -1
			plr.flp = true
		else
			plr.gvx = 0
		end
	if(btnp(⬆️) and energy_bar.val == energy_bar.full_val) then
			gen_jump()
			if(plr.vy < 0) then // if we initiated a jump
				energy_bar.decr = true
			end
		end
end


function bat_state()
	local prev_state = plr.state
	if(not(on_ground(plr))) then
		plr.state = "jump"
	else
		plr.state = "idle"
	end
	if(plr.state ~= prev_state) then
		set_i(plr,1)
	end
end

function bat_vel()
	local grnd = on_ground(plr)
	if(btnp(⬆️) and energy_bar.val > 0) then
		plr.vy = get_jump_height(plr)
		energy_bar.val -= 5
	end
	
	// refactor
	if(grnd and energy_bar.val < energy_bar.full_val) then
		energy_bar.val += 5
	end
	
	if(btn(➡️) and not(grnd)) then
		plr.gvx = get_speed(plr)
		plr.flp = false
	elseif(btn(⬅️) and not(grnd)) then
		plr.gvx = get_speed(plr) * -1
		plr.flp = true
	elseif(btn(⬅️)) then
		plr.flp = true
		plr.gvx = 0 // refactor
	elseif(btn(➡️)) then
		plr.flp = false
		plr.gvx = 0 // refactor
	else
		plr.gvx = 0
	end
end

function gen_state()
	local grnd = on_ground(plr)
	local prev_state = plr.state
	if((btn(➡️) or btn(⬅️)) and grnd) then
		plr.state = "walk"
	elseif(btn(⬆️) or plr.vy < 0) then
		plr.state = "jump"
	elseif(plr.vy > 0 and not(grnd)) then
		plr.state = "fall"
	elseif (plr.vy == 0 and not(grnd)) then
		plr.state = "midjump"
	else
		plr.state = "idle"
	end
	if(plr.state ~= prev_state) then
		set_i(plr,1)
	end
end

function charge()
	if(energy_bar.val == energy_bar.full_val) then
		energy_bar.decr = true
	end
end

function x_listener()
	if(btnp(❎)) then
		local x_fn = plr[plr.mode].x_fn
		x_fn()
	end
end

function human_update()
	gen_state()
	gen_vel()
end

function rhino_update()
	decr_cont(energy_bar,10)
	if(energy_bar.decr) then
		create_trail(plr,⬅️,10)
	end
	gen_state()
	rhino_vel()
end

function frog_update()
	decr_cont(energy_bar,5)
	gen_state()
	frog_vel()
	if(plr.vy < 0) then
		create_trail(plr,⬆️,2)
	end
end

function bat_update()
	bat_state()
	bat_vel()
end


