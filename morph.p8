function _init()
	x_offset = 0
	y_offset = 0
	init_player()
	init_env()
	init_menu()
end

function _update()
	if(not(menu_up)) then
		set_state()
		plr_vel()
		move(plr)
		animate_frame(plr)
	else 
		menu_logic()
	end
	menu_listener()
end

function _draw()
	cls()
	map(x_offset, y_offset)
	if(not(menu_up)) then
		draw_spr(plr)
		//print(plr.gvx)
		//print(plr.vx)
	else
		draw_menu()
	end
end
