function set_bar_val(bar, val)
	bar.val = val
end

function get_bar_val(bar)
	return bar.val
end

function draw_bar(bar)
	if(not(bar.vis)) then
		return
	end
	local bar_w = bar.full*bar.val/100
	rect(
		bar.x1,
		bar.y1,
		bar.x2,
		bar.y2,
		6
	)
	if(bar.val <= 0) then
		bar.val = 0
	else
		rectfill(
			bar.x1 + 2,
			bar.y1 + 2,
			bar.x1 + 2 + bar_w,
			bar.y2 - 2,
			bar.clr
		)
	end
end

function decr_cont(bar,decr)
	if(bar.decr) then
		bar.val -= decr
		if(bar.val <= 0) then
			bar.val = 0
			bar.decr = false
		end
	elseif(bar.val < bar.full_val) then
		bar.val += decr
		if(bar.val >= bar.full_val) then
			bar.val = bar.full_val
		end
	end
end
