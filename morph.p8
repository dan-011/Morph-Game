function _init()
	x_offset = 0
	y_offset = 0
	init_player()
	init_env()
	init_menu()
	init_bars()
	init_trail()
	flag = ""
	camera_x = plr.x
end

function _update()
	if(not(menu_up)) then
		update_obj(plr)
		move(plr)
		animate_frame(plr)
		x_listener()
		update_trail()
	else
		menu_logic()
	end
	menu_listener()
	
	camera_x = plr.x
end

function _draw()
	cls()
	map(x_offset, y_offset)
	if(not(menu_up)) then
		draw_spr(plr)
		draw_bar(energy_bar)
		draw_bar(health_bar)
		draw_trail()
	else
		draw_menu()
	end
	print(plr.vx)
	print(plr.vy)
end
