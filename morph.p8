--animations

function animate_frame(obj)
	local dt = get_dt(obj)
	local t = get_t(obj)
	local i = get_i(obj)
	local n = get_n(obj)
	if time() - t >= dt then
		if(i >= n) then
			set_i(obj,1)
		else
			set_i(obj,i+1)
		end
		set_t(obj,time())
	end
end
